int select(int arr[], int i)
{
    // code here such that selecionSort() sorts arr[]
    return 0;
}


void selectionSort(int arr[], int n)
{
  //code here
  int min,j,temp;
  for(int i=0;i<n-1;i++)
  {
      min=i;
      for(j=i+1;j<n;j++)
      {
          if(arr[min]>arr[j])
          {
              min=j;
          }
      }
      if(min!=i)
      {
          temp=arr[min];
          arr[min]=arr[i];
          arr[i]=temp;
      }
  }
}
